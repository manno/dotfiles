alias passiv set ftp:passive-mode on
alias nopassiv set ftp:passive-mode off
alias ll ls -alCF
alias l "cls -l"
alias qget "queue mget"
alias qmirror "queue mirror"
alias qput "queue mirror -R"
alias qrm "queue rm"
